  "NOTE: We replace absolute links with a use-relative-links-to-md-files-only since these are (nearly) always mistakes and should link to the relative .md file instead": "",
  "replacementPatterns": [
    {
      "pattern": "guidebook.civicactions.com",
      "replacement": "use-relative-links-to-md-files-only.guidebook.civicactions.com"
    },
    {
      "pattern": "github.com/CivicActions/guidebook/tree/",
      "replacement": "use-relative-links-to-md-files-only.github.com/CivicActions/guidebook/tree/"
    }
  ],
  "httpHeaders": [
    {
      "urls": [
        "https://github.com/",
        "https://guides.github.com/",
        "https://help.github.com/",
        "https://docs.github.com/"
      ],
      "headers": {
        "Accept-Encoding": "zstd, br, gzip, deflate"
      }
    }
  ]
}
